<% if (item.type === "image") { %>
<img src="http://localhost:3000/<%= item.content %>" width="300" alt="image" />
<% } else if (item.type === "video") { %>
<video width="320" controls>
  <source src="http://localhost:3000/<%= item.content %>" type="video/mp4" />
  Your browser does not support the video tag.
</video>
<% } else { %>
    <%= item.content %>
<% } %>

